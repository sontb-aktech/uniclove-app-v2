<resources xmlns:tools="http://schemas.android.com/tools">

    <!-- Base application theme. -->
    <style name="AppTheme" parent="Theme.AppCompat.Light.NoActionBar">
        <!-- Customize your theme here. -->
        <item name="android:textColor">#000000</item>
        <item name="android:ambientShadowAlpha" tools:targetApi="lollipop">0.04</item>
        <item name="android:spotShadowAlpha" tools:targetApi="lollipop">0.04</item>
        <item name="android:windowLightNavigationBar" tools:targetApi="27">true</item>
        <item name="android:navigationBarColor">@android:color/black</item>
        <item name="android:forceDarkAllowed" tools:targetApi="q">false</item>
        <item name="android:colorEdgeEffect">#3D3D3D</item>
        <item name="android:editTextBackground">@drawable/edit_text</item>
        <item name="enforceNavigationBarContrast">false</item>
        <item name="android:windowFullscreen">false</item>
        <item name="android:colorPrimary">#7E56F7</item>
        <item name="android:windowBackground">@drawable/launch_background</item>
    </style>

</resources>