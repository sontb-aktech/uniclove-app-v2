    "android": "react-native run-android",
    "ios": "react-native run-ios",
    "lint": "eslint .",
    "start": "react-native start",
    "test": "jest",
    "android-dev": "cross-env ENVFILE=.env.dev react-native run-android",
    "android-dev-release": "cross-env ENVFILE=.env.dev react-native run-android --variant=release",
    "android-std": "cross-env ENVFILE=.env.std react-native run-android",
    "android-prod": "cross-env ENVFILE=.env.prod react-native run-android",
    "android-release-dev": "cd android && cross-env ENVFILE=.env.dev ./gradlew assembleRelease && cd ..",
    "android-release-std": "cd android && cross-env ENVFILE=.env.std ./gradlew assembleRelease && cd ..",
    "android-release-prod": "cd android && cross-env ENVFILE=.env.prod ./gradlew assembleRelease && cd ..",
    "android-release-prod-aab": "cd android && cross-env ENVFILE=.env.prod ./gradlew bundleRelease && cd ..",
    "ios-dev": "react-native run-ios --scheme \"Academy Dev\"",
    "ios-std": "react-native run-ios --scheme \"Academy Std\"",
    "ios-prod": "react-native run-ios --scheme \"Academy Prod\"",
    "adb-reverse": "adb reverse tcp:8081 tcp:8081",
    "postinstall": "patch-package",
    "install": "postinstall-postinstall",
    "create-patch": "npx --yes patch-package --exclude 'android/build/|xcodeproj'",
    "check-update": "npx npm-check-updates -i",
    "link-asset": "npx react-native-asset"
  },
